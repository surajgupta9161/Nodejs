<%- include("../includes/head") %>

    <body>
        <h1>This account is @<%= data.name %>
        </h1>
        <button>Follow</button>
        <button>Message</button>

        <h4>Followers <%=data.followers %>
        </h4>
        <h4>Following <%=data.following %>
        </h4>

        <% for(let post of data.posts) {%>
            <img src="<%= post.image %>" alt="photo">
            <p>Likes: <%= post.likes %> &nbsp; &nbsp;Comments: <%= post.comments %>
            </p>
            <% } %>

    </body>

    </html>